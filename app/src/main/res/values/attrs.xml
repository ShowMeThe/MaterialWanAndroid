<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="JumpAnimView">
        <attr name="text_color" format="color|reference"/>
    </declare-styleable>

    <declare-styleable name="ShakingImageView">
        <attr name="shake_orientation" format="enum">
            <enum name="HORIZONTAL" value="0"/>
            <enum name="VERTICAL" value="1"/>
        </attr>
        <attr name="shake_radius" format="dimension"/>
        <attr name="shake_auto" format="boolean"/>
        <attr name="shake_duration" format="integer"/>
    </declare-styleable>

    <declare-styleable name="IconSwitch">
        <attr name="defaultTint" format="color"/>
        <attr name="transitionTint" format="color"/>
        <attr name="clickToTransition" format="boolean"/>
        <attr name="transitionDuration" format="integer"/>
        <attr name="iconDefault" format="reference"/>
        <attr name="iconTransition" format="reference"/>
    </declare-styleable>

    <declare-styleable name="WaveView">
        <attr name="wakeWidth" format="dimension"/>
        <attr name="wakeHeight" format="dimension"/>
        <attr name="wakeColor" format="color"/>
        <attr name="wakeCount" format="integer"/>
    </declare-styleable>
</resources>